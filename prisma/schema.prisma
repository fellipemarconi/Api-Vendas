generator kysely {
    provider        = "prisma-kysely"
    output          = "../src/common/infrastructure/database/"
    fileName        = "types.ts"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(dbgenerated("gen_random_uuid()"))
  username String   @unique
}
